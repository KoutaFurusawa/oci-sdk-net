  <ItemGroup>
    <None Remove="Resources\endpoints.json" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Resources\endpoints.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="BouncyCastle.NetCore" Version="1.8.3" />
    <PackageReference Include="ErikEJ.EntityFrameworkCore.DgmlBuilder" Version="2.1.1" />
